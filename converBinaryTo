// fa(7)
let  counter=0
function fa(a,b){
let c
    counter++
    if(b==3){
        return (a*a*a)
    }
    else{
        c=fa(a,Math.floor(b/3))
        return(c*c*c)
    }
}
fa(4,81)
console.log(counter)